{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "extends":"airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "quotes": [2, "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "linebreak-style": ["off"],
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./app"],
      },
    },
  },
}
